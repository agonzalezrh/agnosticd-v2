  name: "{{ network_name }}"
  namespace: "{{ ai_ocp_namespace }}"
spec:
  config: '{"cniVersion":"0.3.1","type":"ovn-k8s-cni-overlay","topology":"layer2","name": "{{ network_name }}", "mtu": {{ ai_network_mtu }}, "netAttachDefName": "{{ ai_ocp_namespace }}/{{ network_name }}"}'
